[user]
    name = Antonio Gurgel
    email = antonio@goorzhel.com
    signingkey = 222E8DC40C171D68
[color]
    ui = auto
[push]
    default = matching
[alias]
    a = add -v
    aa = add -v .
    aaa = add -Av .
    c = commit
    co = checkout
    cm = commit -m
    d = diff
    dh = diff HEAD
    dh1 = diff HEAD~1
    rv = remote -v
    st = status
    ps = push
    pupm = pull upstream master
    pom = push origin master
    p = pull --rebase --ff-only
    diff-fetch = diff ..FETCH_HEAD
    revert-file = checkout HEAD --
    revert-file-server = checkout FETCH_HEAD --
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    lgg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --name-only
    ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    ld = log --pretty=format:"%C(yellow)%h\\ %C(green)%ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short --graph
    ls = log --pretty=format:"%C(green)%h\\ %C(yellow)[%ad]%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=relative
    serve = daemon --verbose --export-all --base-path=.git --reuseaddr --strict-paths .git/
[core]
    excludesfile = ~/.gitignore_global
[github]
    user = Goorzhel
